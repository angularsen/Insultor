{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "jsx": "react",
    "types": [
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "*": ["*", "./@types/*"]
    }
  },
  "exclude": [
    "node_modules",
    "www"
  ]
}