{
  "name": "insultor",
  "version": "0.1.0",
  "description": "Delivers insults to your face, based on your face.",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf www/bundle.js*",
    "compile": "webpack",
    "prebuild": "npm run clean -s",
    "build": "webpack --config webpack.prod.js",
    "build-dev": "webpack --config webpack.dev.js",
    "start": "node server.js",
    "deploy": "npm run build && surge -p www",
    "deploy-dev": "npm run build-dev && surge -p www",
    "test": "karma start"
  },
  "keywords": [
    "insults",
    "for",
    "your",
    "face",
    "using",
    "react",
    "webpack",
    "babel",
    "Microsoft Face API"
  ],
  "author": "Andreas Gullberg Larsen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/angularsen/insultor"
  },
  "dependencies": {
    "express": "^4.16.2",
    "javascript-state-machine": "^3.0.1",
    "moment": "^2.19.2",
    "moment-mini": "^2.18.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.2",
    "@types/node": "^8.0.53",
    "@types/react": "^16.0.20",
    "@types/react-dom": "^16.0.2",
    "awesome-typescript-loader": "^3.4.0",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "jasmine": "^2.8.0",
    "jasmine-core": "^2.8.0",
    "jasmine-reporters": "^2.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-typescript": "^3.0.8",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.3",
    "style-loader": "^0.19.0",
